{
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000, immutable"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": [
                {
                    "src": "/[^.]+",
                    "dest": "/",
                    "status": 200
                }
            ],
            "dest": "/index.html"
        }
    ]
}